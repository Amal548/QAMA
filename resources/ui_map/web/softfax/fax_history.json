{
  "title": {
    "locator": {"xpath": "//h1[@id = 'pageTitle' and contains(text(), 'Fax History')]"}
  },
  "menu_btn":{
    "locator":{"css selector": "button#menuButton"}
  },
  "menu_select_btn":{
    "locator": {"css selector": "a#selectFaxesMenu"}
  },
  "edit_cancel_txt": {
    "locator": {"xpath":  "//span[@id = 'rightAction']/a[contains(text(), 'Cancel')]"}
  },
  "edit_delete_btn": {
    "locator": {"css selector": "a#deleteFax"}
  },
  "edit_export_log_btn": {
    "locator": {"css selector": "a#exportLog"}
  },
  "send_tab_btn": {
    "_comment": "Webview, click() unsucessfully on iOS app so using Native App locator",
    "locator": {"css selector": "div#sentTab",
                "name": "Sent"}
  },
  "empty_sent_fax_title": {
    "locator": {"css selector":  "h2#emptySentFaxesTitle"}
  },
  "empty_sent_fax_message": {
    "locator": {"css selector":  "p#emptySentFaxesMessage"}
  },
  "draft_tab_btn": {
    "_comment": "Webview, click() unsucessfully on iOS app so using Native App locator",
    "locator": {"css selector": ["div#drafTab", "div#draftTab"],
                "name": "Drafts"}
  },
  "empty_draft_message": {
    "locator": {"css selector":  "p#emptyDraftsMessage"}
  },
  "sent_history_record_cell": {
    "_comment": "Native App locator is used for swipe ()",
    "locator": {"xpath":  ["//android.view.View[contains(@resource-id, 'outboundItem')]",
                           "//span[contains(@id,'faxRecipient')]", "//div[contains(@id, 'outboundItem')]", "//XCUIElementTypeOther/XCUIElementTypeStaticText[@contains(@name,'+1')]"]}
  },
  "draft_history_record_cell": {
    "_comment": "Native App locator is used for swipe ()",
    "locator": {
      "name": "Drafts",
      "xpath":  ["//android.view.View[contains(@resource-id, 'swipeableListItem')]",
                 "//span[contains(@id,'draft')]", "//span[contains(@id, 'swipeableListItem')]",
                 "//span[contains(@id,'swipeableOutboundItem1')]","//XCUIElementTypeOther/XCUIElementTypeStaticText[contains(@name,'Drafts')]"]}

  },
  "history_record_phone_number":{
    "_comment": "it is common usage for sent and draft tabs",
    "locator": {"xpath": "//*[contains(text(), '{}')]"}
  },
  "sent_record_status_icon": {
    "locator": {"xpath":  "//img[contains(@id, 'icon')]"}
  },
  "sent_record_number_pages_txt": {
    "locator": {"xpath": "//span[contains(@id, 'faxPages')]"}
  },
  "sent_record_date_txt": {
    "locator": {"xpath": "//span[contains(@id, 'faxDate')]"}
  },
  "failed_icon": {
    "locator": {"xpath": "//img[@src='/sws/static/img/error.cbd4ec9a.svg']"}
  },
  "successful_icon": {
    "locator": {"xpath": "//img[@src='/sws/static/img/history-sent.0c4eea1f.svg']"}
  },
  "processing_icon": {
    "locator": {"xpath": "//img[@src='/sws/static/img/history-loading.8d85a0ec.svg']"}
  },
  "draft_record_label": {
    "locator": {"xpath": "//span[contains(@id, 'draftLabel')]"}
  },
  "draft_record_phone_txt": {
    "locator": {"xpath": "//span[contains(@id, 'draftPhone')]"}
  },
  "draft_record_number_pages_txt": {
    "locator": {"xpath": "//div[contains(@id, 'draftNumPages')]"}
  },
  "draft_record_date_txt": {
    "locator": {"xpath": "//span[contains(@id, 'draftDate')]"}
  },
  "record_delete_btn": {
    "_comment": "To webview, click() is unsucessfully without error message (return True). Native App locator is for click() action",
    "locator": {"text":  "Delete"}
  },
  "delete_popup_cancel_btn": {
    "locator": {"css selector": "button#cancelBtn"}
  },
  "delete_popup_delete_btn": {
    "locator": {"css selector": "button#deleteBtn"}
  },
  "record_export_btn": {
    "_comment": "To webview, click() is unsucessfully without error message (return True). Native App locator is for click() action",
    "locator": {"text":  "Export"}
  },
  "compose_new_fax_btn": {
    "locator": {"css selector":  "button#composeFaxBtn"}
  }
}